#ifndef SRC_PIXEL_DATA_H_
#define SRC_PIXEL_DATA_H_

typedef struct {
	uint16_t blue, green, red;
} Pixel;

typedef struct {
	Pixel pixels[96];
} Layer;

typedef struct {
	Layer layers[24];
} Frame;

#endif /* SRC_PIXEL_DATA_H_ */
